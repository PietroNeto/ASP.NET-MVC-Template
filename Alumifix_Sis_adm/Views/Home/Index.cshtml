<div class="card text-center">
    <div class="card-header">
        Hosts Up
    </div>
    <div class="card-body">
    <table id="example" class="display" style="width:100%">
    </table>
    </div>
    <div class="card-footer text-body-secondary">
    2 days ago
    </div>
</div>

<script>
    var HostsUpjson;
    $(document).ready(function () {
        $.get("/LocalMonitor/IpLIst/192.168.1.1+24", function(response) {
            HostsUpjson = response
        }).then(function(){
            let HostsUp = $('#example').DataTable({
                "data": HostsUpjson,
                "columns":[
                    { title: 'Ip', data: "ip"},
                    { title: 'MAC', data: "mac"},
                    { title: 'Name', data: "name"},
                ],
                //order: [[1, 'asc']],
            });
        })
    });
</script>